{
  "name": "pillar",
  "version": "1.0.0",
  "description": "Discord bot that can play music, setup reminders, managing welcome and leaving messages.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p .",
    "fix": "concurrently \"cpx package-hotfixes/index.js node_modules/miniget/dist\" \"cpx package-hotfixes/info.js node_modules/ytdl-core/lib\"",
    "postinstall": "npm run fix"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Erik Szab√≥",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "axios": "^0.21.0",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^4.2.7",
    "monk": "^7.3.2",
    "node-schedule": "^1.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "ytdl-core": "^4.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/monk": "^6.0.0",
    "@types/node": "^14.6.4",
    "@types/node-schedule": "^1.3.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.2.6",
    "concurrently": "^5.3.0",
    "cpx": "^1.5.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
